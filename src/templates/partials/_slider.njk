  <script>
  $(window).load(function() {
      $('#swipeslider').swipeslider({
        sliderHeight: '43%',
        autoPlayTimeout: 5000
      });
  });
</script>

{% set slides = {
  location: '/gallery/',
  previewPrefix: '_slider',
  type: '.jpg',
  collection:[{
  alt: 'Tiny vacation',
  name: '0_tiny_vacation'
  },{alt: 'Little Red Riding Hood',
  name: '4_little_red_riding_hood'
  },{alt: 'Summer Days',
  name: '3_summer_days'
  },{alt: 'Lucy Wilde from Despicable me 2',
  name: '10_lucy_wilde'
  },{alt: 'Stylized oscilloscope',
  name: 'oscilloscope'
  },{alt: 'Lang from Yie Ar Kung-Fu',
  name: '5_lang_yie_ar_kungfu'
  }
]} %}


 <div class="slider-container">
  <header class="slider-header">
  <h2 class="block-title">Highlights</h2>
  </header>
  <figure id="swipeslider" class="swipeslider">
    <ul class="sw-slides">
      {% for slide in slides.collection %}
        <li class="sw-slide">
          <figure class="sw-content">
          <a class="lsb-preview" data-lsb-group="slider" 
             href="{{slides.location + slide.name + slides.type}}">
            <img src="{{slides.location + slide.name + slides.previewPrefix + slides.type}}" 
              alt="{{slide.alt}}">
          </a>
          </figure>
        </li>
      {% endfor %}
    </ul>
  </figure>
</div>